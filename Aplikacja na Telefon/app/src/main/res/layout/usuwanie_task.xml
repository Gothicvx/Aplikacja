<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/b111"
        android:layout_width="327dp"
        android:layout_height="112dp"
        android:layout_margin="0dp"
        android:background="#FFFFFF"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="249dp"
        android:layout_height="29dp"
        android:layout_marginTop="16dp"
        android:text="Czy na pewno chcesz usunąć?"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/b111"
        app:layout_constraintStart_toStartOf="@+id/b111"
        app:layout_constraintTop_toTopOf="@+id/b111"
        android:textSize="16dp"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tak"
        app:layout_constraintBottom_toBottomOf="@+id/b111"
        app:layout_constraintEnd_toStartOf="@+id/button6"
        app:layout_constraintStart_toStartOf="@+id/b111"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:onClick="usuwanie"/>

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nie"
        android:onClick="wrocTask"
        app:layout_constraintBottom_toBottomOf="@+id/b111"
        app:layout_constraintEnd_toEndOf="@+id/b111"
        app:layout_constraintHorizontal_bias="0.807"
        app:layout_constraintStart_toStartOf="@+id/b111"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.473" />
</androidx.constraintlayout.widget.ConstraintLayout>
